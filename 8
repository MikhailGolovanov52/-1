
#include <iostream> using namespace std; int main()
{
int a = 50; // целочисленная переменная а
int &b = а; // ссылка в
cout << "a\t b\n";
cout << a <<
a++; // 1
-
альтернативное имя для переменной а
"\t" << b << endl;
cout << a << "\t" << b << endl;
b++; // 2
cout << a << "\t" << b << endl; return 0;
}
