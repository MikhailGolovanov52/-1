
Дан массив из !! действительных чисел (и <100). Написать программу для подсчета
суммы этих чисел.
#include <iostream>
using namespace std;
int main()
{
float a[100];
int n;
cout << "n";
cin >> n;
float s = 0;
}
for (int i =
0; i < n; ++i)
{
cout << "a" << i << "] = ";
cin>> a[i];
5 += a[i]; // добавление значения элемента массива к сумме
}
cout << "s =
<< s << endl;
return 0;
